# Configurare pentru sursa de date primară
spring.datasource.primary.url=jdbc:mysql://localhost:3306/formare_db
spring.datasource.primary.username=root
spring.datasource.primary.password=root

# Configurare pentru EntityManagerFactory-ul asociat sursa de date primară
spring.jpa.primary.hibernate.ddl-auto=update
spring.jpa.primary.show-sql=true
spring.jpa.primary.properties.hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
spring.jpa.primary.properties.hibernate.jdbc.lob.non_contextual_creation=true
spring.jpa.primary.persistence-unit-name=primary

# Configurare pentru sursa de date secundară
spring.datasource.secondary.url=jdbc:mysql://codeplus.ddns.net:3306/transfer
spring.datasource.secondary.username=root
spring.datasource.secondary.password=R@0t
spring.datasource.secondary.driver-class-name=com.mysql.cj.jdbc.Driver

# Configurare pentru EntityManagerFactory-ul asociat sursa de date secundară
spring.jpa.secondary.hibernate.ddl-auto=update
spring.jpa.secondary.show-sql=true
spring.jpa.secondary.properties.hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
spring.jpa.secondary.properties.hibernate.jdbc.lob.non_contextual_creation=true
spring.jpa.secondary.persistence-unit-name=secondary
